{
  "name": "tv-ptogram",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "stylus": "stylus -u autoprefixer-stylus --compress ./src/style.styl -o ./dist/style.css",
    "jade": "jade ./src/index.jade -o ./dist/",
    "rollup": "rollup -c",
    "build": "npm run jade & npm run stylus & npm run rollup",
    "watch-jade": "nodemon --ext jade --watch ./src/ --exec 'npm run jade'",
    "watch-stylus": "nodemon --ext styl --watch ./src/ --exec 'npm run stylus'",
    "watch-rollup": "nodemon --ext js --watch ./src/ --exec 'npm run rollup'",
    "watch": "npm run watch-jade & npm run watch-stylus & npm run watch-rollup",
    "start-watch": "npm run watch & npm run start",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer-stylus": "^0.9.2",
    "express": "^4.13.4",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "jade": "^1.11.0",
    "nodemon": "^1.9.1",
    "rollup": "^0.26.0",
    "rollup-plugin-babel": "^2.4.0",
    "stylus": "^0.54.2"
  }
}
